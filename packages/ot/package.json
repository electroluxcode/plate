{
  "name": "@platejs/ot",
  "version": "1.0.0",
  "description": "Operational Transform plugin for Plate - ShareDB integration",
  "keywords": [
    "plate",
    "plugin",
    "slate",
    "ot",
    "operational-transform",
    "sharedb",
    "collaboration"
  ],
  "homepage": "https://platejs.org",
  "bugs": {
    "url": "https://github.com/udecode/plate/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/udecode/plate.git",
    "directory": "packages/ot"
  },
  "license": "MIT",
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "module": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./react": {
      "types": "./dist/react/index.d.ts",
      "import": "./dist/react/index.mjs",
      "module": "./dist/react/index.mjs",
      "require": "./dist/react/index.js"
    },
    "./demo": {
      "types": "./dist/demo/index.d.ts",
      "import": "./dist/demo/index.mjs",
      "module": "./dist/demo/index.mjs",
      "require": "./dist/demo/index.js"
    }
  },
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist/**/*"
  ],
  "scripts": {
    "brl": "yarn p:brl",
    "build": "yarn p:build",
    "build:watch": "yarn p:build:watch",
    "clean": "yarn p:clean",
    "lint": "yarn p:lint",
    "lint:fix": "yarn p:lint:fix",
    "test": "yarn p:test",
    "test:watch": "yarn p:test:watch",
    "typecheck": "yarn p:typecheck",
    "demo:dev": "vite",
    "demo:build": "vite build",
    "demo:preview": "vite preview",
    "server": "node demo/server/server.js"
  },
  "dependencies": {
    "@teamwork/websocket-json-stream": "2.0.0",
    "ottype-slate-test": "0.1.0",
    "reconnecting-websocket": "^4.4.0",
    "sharedb": "5.2.2",
    "websocket-json-stream": "0.0.3",
    "ws": "^8.18.0"
  },
  "devDependencies": {
    "@types/ws": "^8.5.10",
    "platejs": "workspace:^",
    "vite": "^5.0.0"
  },
  "peerDependencies": {
    "platejs": ">=49.2.12",
    "react": ">=18.0.0",
    "react-dom": ">=18.0.0",
    "slate": ">=0.100.0",
    "slate-history": ">=0.100.0",
    "slate-react": ">=0.100.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
